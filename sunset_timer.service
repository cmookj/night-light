[Unit]
Description=Sunset Timer LED Lighting

Wants=network.target
After=syslog.target network-online.target

[Service]
Type=simple
WorkingDirectory=/home/cmookj/sunset_timer
ExecStart=/usr/local/bin/sunset_timer.sh
Restart=on-failure
RestartSec=10
KillMode=process

[Install]
WantedBy=multi-user.target

